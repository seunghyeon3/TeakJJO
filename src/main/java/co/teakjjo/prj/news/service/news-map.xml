<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.teakjjo.prj.news.service.NewsMapper">

<select id="newsList" resultType="co.teakjjo.prj.news.service.NewsVO">
	SELECT * FROM NEWSBOARD
</select>
<select id="newsMyList" parameterType="String" resultType="co.teakjjo.prj.news.service.NewsVO">
	SELECT * FROM NEWSBOARD WHERE MEMBER_ID = #{member_id}
</select>
<insert id="newsInsert" parameterType="co.teakjjo.prj.news.service.NewsVO">
INSERT INTO NEWSBOARD(MEMBER_ID,MEMBER_NAME,NEWSBOARD_ID,MEMBER_COMPANY,NEWSBOARD_TITLE,NEWSBOARD_CONTENT,NEWSBOARD_HIT,NEWSBOARD_PFILE,NEWSBOARD_PICTURE)
VALUES(#{member_id},#{member_name},#{newsboard_id},#{member_company},#{newsboard_title},#{newsboard_content},#{newsboard_hit},#{newsboard_pfile},#{newsboard_picture})
</insert>
<delete id="newsDelete" parameterType="String">
DELETE FROM NEWSBOARD WHERE NEWSBOARD_ID = #{newsboard_id}
</delete>
</mapper>